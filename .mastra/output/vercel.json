{
  "version": 2,
  "installCommand": "npm install --omit=dev",
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "index.mjs",
          "index2.mjs",
          "instrumentation.mjs",
          "mastra.mjs",
          "package.json",
          "pnpm-lock.yaml",
          "telemetry-config.mjs",
          "tools",
          "tools.mjs",
          "tools/f2c2ad62-7a53-4a1f-bb4c-cb509927cd76.mjs"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.mjs"
    }
  ]
}