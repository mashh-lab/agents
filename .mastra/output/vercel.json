{
  "version": 2,
  "installCommand": "npm install --omit=dev",
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "index.mjs",
          "instrumentation.mjs",
          "mastra.mjs",
          "package.json",
          "pnpm-lock.yaml",
          "telemetry-config.mjs",
          "tools",
          "tools.mjs",
          "tools/f893db99-50d3-423e-b085-d2c8ef9ea05a.mjs"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.mjs"
    }
  ]
}